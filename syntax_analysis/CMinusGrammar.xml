<?xml version="1.0" encoding="UTF-8"?>
<grammar name="G">
    <terminalsymbols>
        <term name="Identifier"/>
        <term name="LeftBracket"/>
        <term name="RightBracket"/>
        <term name="Number"/>
        <term name="SemiColon"/>
        <term name="int"/>
        <term name="void"/>
        <term name="Colon"/>
        <term name="LeftParen"/>
        <term name="RightParen"/>
        <term name="LeftBrace"/>
        <term name="RightBrace"/>
        <term name="if"/>
        <term name="else"/>
        <term name="while"/>
        <term name="return"/>
        <term name="Equal"/>
        <term name="LessOrEqual"/>
        <term name="Less"/>
        <term name="Greater"/>
        <term name="GreaterOrEqual"/>
        <term name="DoubleEqual"/>
        <term name="NotEqual"/>
        <term name="Plus"/>
        <term name="Minus"/>
        <term name="Mul"/>
        <term name="Div"/>
        <term name="e"/>
    </terminalsymbols>
    <nonterminalsymbols>
        <nonterm name="program"/>
        <nonterm name="declarationList"/>
        <nonterm name="declarationListNew"/>
        <nonterm name="declaration"/>
        <nonterm name="varDeclaration"/>
        <nonterm name="typeSpecifier"/>
        <nonterm name="funDeclaration"/>
        <nonterm name="params"/>
        <nonterm name="paramList"/>
        <nonterm name="paramListNew"/>
        <nonterm name="param"/>
        <nonterm name="compoundStmt"/>
        <nonterm name="localDeclarations"/>
        <nonterm name="localDeclarationsNew"/>
        <nonterm name="statementList"/>
        <nonterm name="statementListNew"/>
        <nonterm name="statement"/>
        <nonterm name="expressionStmt"/>
        <nonterm name="selectionStmt"/>
        <nonterm name="iterationStmt"/>
        <nonterm name="returnStmt"/>
        <nonterm name="expression"/>
        <nonterm name="var"/>
        <nonterm name="simpleExpression"/>
        <nonterm name="relop"/>
        <nonterm name="additiveExpression"/>
        <nonterm name="additiveExpressionNew"/>
        <nonterm name="addop"/>
        <nonterm name="term"/>
        <nonterm name="termNew"/>
        <nonterm name="mulop"/>
        <nonterm name="factor"/>
        <nonterm name="call"/>
        <nonterm name="args"/>
        <nonterm name="argsList"/>
        <nonterm name="argsListNew"/>
    </nonterminalsymbols>
    <productions>

        <production>
            <lhs name="program"/>
            <rhs>
                <symbol type="nonterm" name="declarationList"/>
            </rhs>
        </production>

        <production>
            <lhs name="declarationList"/>
            <rhs>
                <symbol type="nonterm" name="declaration"/>
                <symbol type="nonterm" name="declarationListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="declarationListNew"/>
            <rhs>
                <symbol type="nonterm" name="declaration"/>
                <symbol type="nonterm" name="declarationListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="declarationListNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="declaration"/>
            <rhs>
                <symbol type="nonterm" name="varDeclaration"/>
            </rhs>
        </production>

        <production>
            <lhs name="declaration"/>
            <rhs>
                <symbol type="nonterm" name="funDeclaration"/>
            </rhs>
        </production>

        <production>
            <lhs name="varDeclaration"/>
            <rhs>
                <symbol type="nonterm" name="typeSpecifier"/>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="varDeclaration"/>
            <rhs>
                <symbol type="nonterm" name="typeSpecifier"/>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="LeftBracket"/>
                <symbol type="term" name="Number"/>
                <symbol type="term" name="RightBracket"/>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="typeSpecifier"/>
            <rhs>
                <symbol type="term" name="int"/>
            </rhs>
        </production>

        <production>
            <lhs name="typeSpecifier"/>
            <rhs>
                <symbol type="term" name="void"/>
            </rhs>
        </production>

        <production>
            <lhs name="funDeclaration"/>
            <rhs>
                <symbol type="nonterm" name="typeSpecifier"/>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="params"/>
                <symbol type="term" name="RightParen"/>
                <symbol type="nonterm" name="compoundStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="params"/>
            <rhs>
                <symbol type="nonterm" name="paramList"/>
            </rhs>
        </production>

        <production>
            <lhs name="params"/>
            <rhs>
                <symbol type="term" name="void"/>
            </rhs>
        </production>

        <production>
            <lhs name="paramList"/>
            <rhs>
                <symbol type="nonterm" name="param"/>
                <symbol type="nonterm" name="paramListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="paramListNew"/>
            <rhs>
                <symbol type="term" name="Colon"/>
                <symbol type="nonterm" name="param"/>
                <symbol type="nonterm" name="paramListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="paramListNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="param"/>
            <rhs>
                <symbol type="nonterm" name="typeSpecifier"/>
                <symbol type="term" name="Identifier"/>
            </rhs>
        </production>

        <production>
            <lhs name="param"/>
            <rhs>
                <symbol type="nonterm" name="typeSpecifier"/>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="LeftBracket"/>
                <symbol type="term" name="RightBracket"/>
            </rhs>
        </production>

        <production>
            <lhs name="compoundStmt"/>
            <rhs>
                <symbol type="term" name="LeftBrace"/>
                <symbol type="nonterm" name="localDeclarations"/>
                <symbol type="nonterm" name="statementList"/>
                <symbol type="term" name="RightBrace"/>
            </rhs>
        </production>

        <production>
            <lhs name="localDeclarations"/>
            <rhs>
                <symbol type="nonterm" name="localDeclarationsNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="localDeclarationsNew"/>
            <rhs>
                <symbol type="nonterm" name="varDeclaration"/>
                <symbol type="nonterm" name="localDeclarationsNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="localDeclarationsNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="statementList"/>
            <rhs>
                <symbol type="nonterm" name="statementListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="statementListNew"/>
            <rhs>
                <symbol type="nonterm" name="statement"/>
                <symbol type="nonterm" name="statementListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="statementListNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="statement"/>
            <rhs>
                <symbol type="nonterm" name="expressionStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="statement"/>
            <rhs>
                <symbol type="nonterm" name="compoundStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="statement"/>
            <rhs>
                <symbol type="nonterm" name="selectionStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="statement"/>
            <rhs>
                <symbol type="nonterm" name="iterationStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="statement"/>
            <rhs>
                <symbol type="nonterm" name="returnStmt"/>
            </rhs>
        </production>

        <production>
            <lhs name="expressionStmt"/>
            <rhs>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="expressionStmt"/>
            <rhs>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="selectionStmt"/>
            <rhs>
                <symbol type="term" name="if"/>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="RightParen"/>
                <symbol type="nonterm" name="statement"/>
            </rhs>
        </production>

        <production>
            <lhs name="selectionStmt"/>
            <rhs>
                <symbol type="term" name="if"/>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="RightParen"/>
                <symbol type="nonterm" name="statement"/>
                <symbol type="term" name="else"/>
                <symbol type="nonterm" name="statement"/>
            </rhs>
        </production>

        <production>
            <lhs name="iterationStmt"/>
            <rhs>
                <symbol type="term" name="while"/>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="RightParen"/>
                <symbol type="nonterm" name="statement"/>
            </rhs>
        </production>

        <production>
            <lhs name="returnStmt"/>
            <rhs>
                <symbol type="term" name="return"/>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="returnStmt"/>
            <rhs>
                <symbol type="term" name="return"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="SemiColon"/>
            </rhs>
        </production>

        <production>
            <lhs name="expression"/>
            <rhs>
                <symbol type="nonterm" name="var"/>
                <symbol type="term" name="Equal"/>
                <symbol type="nonterm" name="expression"/>
            </rhs>
        </production>

        <production>
            <lhs name="expression"/>
            <rhs>
                <symbol type="nonterm" name="simpleExpression"/>
            </rhs>
        </production>

        <production>
            <lhs name="var"/>
            <rhs>
                <symbol type="term" name="Identifier"/>
            </rhs>
        </production>

        <production>
            <lhs name="var"/>
            <rhs>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="LeftBracket"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="RightBracket"/>
            </rhs>
        </production>

        <production>
            <lhs name="simpleExpression"/>
            <rhs>
                <symbol type="nonterm" name="additiveExpression"/>
                <symbol type="nonterm" name="relop"/>
                <symbol type="nonterm" name="additiveExpression"/>
            </rhs>
        </production>

        <production>
            <lhs name="simpleExpression"/>
            <rhs>
                <symbol type="nonterm" name="additiveExpression"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="LessOrEqual"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="Less"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="Greater"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="GreaterOrEqual"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="DoubleEqual"/>
            </rhs>
        </production>

        <production>
            <lhs name="relop"/>
            <rhs>
                <symbol type="term" name="NotEqual"/>
            </rhs>
        </production>

        <production>
            <lhs name="additiveExpression"/>
            <rhs>
                <symbol type="nonterm" name="term"/>
                <symbol type="nonterm" name="additiveExpressionNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="additiveExpressionNew"/>
            <rhs>
                <symbol type="nonterm" name="addop"/>
                <symbol type="nonterm" name="term"/>
                <symbol type="nonterm" name="additiveExpressionNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="additiveExpressionNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="addop"/>
            <rhs>
                <symbol type="term" name="Plus"/>
            </rhs>
        </production>

        <production>
            <lhs name="addop"/>
            <rhs>
                <symbol type="term" name="Minus"/>
            </rhs>
        </production>

        <production>
            <lhs name="term"/>
            <rhs>
                <symbol type="nonterm" name="factor"/>
                <symbol type="nonterm" name="termNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="termNew"/>
            <rhs>
                <symbol type="nonterm" name="mulop"/>
                <symbol type="nonterm" name="factor"/>
                <symbol type="nonterm" name="termNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="termNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="mulop"/>
            <rhs>
                <symbol type="term" name="Mul"/>
            </rhs>
        </production>

        <production>
            <lhs name="mulop"/>
            <rhs>
                <symbol type="term" name="Div"/>
            </rhs>
        </production>

        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="term" name="RightParen"/>
            </rhs>
        </production>

        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="nonterm" name="var"/>
            </rhs>
        </production>

        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="nonterm" name="call"/>
            </rhs>
        </production>

        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="Number"/>
            </rhs>
        </production>

        <production>
            <lhs name="call"/>
            <rhs>
                <symbol type="term" name="Identifier"/>
                <symbol type="term" name="LeftParen"/>
                <symbol type="nonterm" name="args"/>
                <symbol type="term" name="RightParen"/>
            </rhs>
        </production>

        <production>
            <lhs name="args"/>
            <rhs>
                <symbol type="nonterm" name="argsList"/>
            </rhs>
        </production>

        <production>
            <lhs name="args"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

        <production>
            <lhs name="argsList"/>
            <rhs>
                <symbol type="nonterm" name="expression"/>
                <symbol type="nonterm" name="argsListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="argsListNew"/>
            <rhs>
                <symbol type="term" name="Colon"/>
                <symbol type="nonterm" name="expression"/>
                <symbol type="nonterm" name="argsListNew"/>
            </rhs>
        </production>

        <production>
            <lhs name="argsListNew"/>
            <rhs>
                <symbol type="term" name="e"/>
            </rhs>
        </production>

    </productions>
    <startsymbol name="program"/>
</grammar>
